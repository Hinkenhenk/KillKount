<Ui xmlns="http://www.blizzard.com/wow/ui/">

  <!-- Section header: e.g. "Outland - Shadowmoon Valley" -->
  <Frame name="KillKountSectionTemplate" virtual="true" enableMouse="false">
    <Size x="1" y="20"/>
    <Layers>
      <Layer level="ARTWORK">
        <FontString name="$parentText" inherits="GameFontHighlightSmall" justifyH="LEFT" maxLines="1" nonspacewrap="true">
          <Anchors>
            <Anchor point="LEFT"  x="8"  y="0"/>
            <Anchor point="RIGHT" x="-8" y="0"/>
          </Anchors>
        </FontString>
      </Layer>
    </Layers>
  </Frame>

  <!-- Data row: left = mob name, right = count -->
  <Frame name="KillKountRowTemplate" virtual="true" enableMouse="false">
    <Size x="1" y="18"/>
    <Layers>
      <Layer level="ARTWORK">
        <FontString name="$parentLeft" inherits="GameFontNormalSmall" justifyH="LEFT" maxLines="1" nonspacewrap="true">
          <Anchors>
            <Anchor point="LEFT"  x="12" y="0"/>
            <Anchor point="RIGHT" x="-60" y="0"/>
          </Anchors>
        </FontString>
        <FontString name="$parentRight" inherits="GameFontNormalSmall" justifyH="RIGHT" maxLines="1" nonspacewrap="true">
          <Anchors>
            <Anchor point="RIGHT" x="-12" y="0"/>
          </Anchors>
        </FontString>
      </Layer>
    </Layers>
  </Frame>

</Ui>